{
  "name": "ra-react-template",
  "version": "1.0.1",
  "description": "React Template by Rokas Anisas",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --mode development",
    "build": "webpack --mode production",
    "lint": "stylelint  **/*.scss && eslint \"src/**/*.{js,jsx,json,md,ts,tsx}\"",
    "lint:fix": "stylelint --fix **/*.scss && prettier --write \"src/**/*.{js,jsx,json,md,ts,tsx}\" && eslint --fix \"src/**/*.{js,jsx,json,md,ts,tsx}\"",
    "test": "jest",
    "generate": "motem templates && npm run lint:fix",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "keywords": [
    "react"
  ],
  "author": "Rokas Anisas",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.2.9",
    "@types/browser-sync-webpack-plugin": "^2.2.1",
    "@types/classnames": "^2.3.0",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.23",
    "@types/mini-css-extract-plugin": "^1.4.3",
    "@types/node": "^15.12.2",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/redux-mock-store": "^1.0.2",
    "@types/stylelint-webpack-plugin": "^2.1.0",
    "@types/webpack": "^5.28.0",
    "autoprefixer": "10.2.6",
    "babel-jest": "^27.0.2",
    "babel-loader": "^8.2.2",
    "browserslist": "^4.16.6",
    "change-case": "^4.1.2",
    "css-loader": "^5.1.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.28.0",
    "eslint-config-ra-react": "^1.0.3",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-loader": "^4.0.2",
    "file-loader": "^6.2.0",
    "fs": "0.0.1-security",
    "html-webpack-plugin": "^5.3.1",
    "husky": "6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.4",
    "mini-css-extract-plugin": "^1.6.0",
    "motem": "^1.0.3",
    "path": "^0.12.7",
    "postcss": "^8.3.4",
    "postcss-loader": "^6.1.0",
    "prefix-commit-message": "^2.0.0",
    "prettier": "^2.3.1",
    "prompts": "^2.4.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-mock-store": "^1.5.4",
    "sass": "^1.34.1",
    "sass-loader": "^10.1.1",
    "sass-resources-loader": "^2.1.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended-scss": "^4.2.0",
