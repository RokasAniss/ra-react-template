    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.0"
  },
  "eslintConfig": {
    "extends": "ra-react",
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "./webpack.config.ts"
        }
      }
    }
  }
}
